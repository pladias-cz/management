{
	"name": "pladias-cz/management",
	"keywords": ["pladias"],
	"type": "project",
	"license": ["MIT"],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/pladias-cz/doctrine-mappings"
    }
  ],
  "require": {
		"php": ">= 8.4",
    "ext-intl": "*",
    "ext-openssl": "*",

		"nette/application": "^3",
		"nette/bootstrap": "^3",
		"nette/caching": "^3",
		"nette/di": "^3",
		"nette/forms": "^3",
		"nette/http": "^3",
		"nette/mail": "^4",
		"nette/robot-loader": "^4",
		"nette/security": "^3",
    "nette/utils": "^4",
    "nettrine/orm": "^0.8",
    "nettrine/dbal": "^0.8.2",
    "nettrine/cache": "^0.3",
    "nettrine/migrations": "^0",
    "latte/latte": "^3",
    "tracy/tracy": "^2",

    "contributte/console": "^0",
    "contributte/console-extra": "^0",
    "contributte/forms-bootstrap": "^0",
    "ublaboo/datagrid": "^v6",

    "68publishers/asset": "^3",
    "68publishers/webpack-encore-bundle": "^3",
    "pladias-cz/doctrine-mappings": "@dev"

  },
	"require-dev": {
		"nette/tester": "^2",
		"symfony/thanks": "^1",
    "contributte/qa": "^0",
		"phpstan/phpstan": "^2"
	},
	"autoload": {
		"psr-4": {
			"App\\": "App"
		}
	},
	"minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "8.4",
      "ext-intl": "3",
      "ext-openssl": "1"
    },
		"allow-plugins": {
			"symfony/thanks": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
